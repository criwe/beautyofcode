<?xml version="1.0" encoding="iso-8859-1" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:extlabel</sheetTitle>
                <type>array</type>
                <el>
                    <cLabel>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:label</label>
                        <config>
                            <type>input</type>
                            <size>50</size>
                            <max>200</max>
                            <eval>trim</eval>
                        </config>
                    </cLabel>
                    <cLang>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:lang</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>

                            <items>
                                <numIndex index="0">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:plain</label>
                                    <value>plain</value>
                                </numIndex>
                            </items>
                            <itemsProcFunc>FelixNagel\Beautyofcode\Configuration\Flexform\LanguageItems->getConfiguredLanguages</itemsProcFunc>
                        </config>
                    </cLang>
                    <cFile>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:file</label>
                        <config>
                            <type>input</type>
                            <size>50</size>
                            <max>200</max>
                            <eval>trim</eval>
                            <wizards>
                                <link>
                                    <type>popup</type>
                                    <module>
                                        <name>wizard_link</name>
                                    </module>
                                    <params>
                                        <blindLinkOptions>folder,mail,page</blindLinkOptions>
                                    </params>
                                </link>
                            </wizards>
                        </config>
                    </cFile>
                </el>
            </ROOT>
        </sDEF>
        <sOPTIONS>
            <ROOT>
                <sheetTitle>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:extlabel.options</sheetTitle>
                <type>array</type>
                <el>
                    <cHighlight>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:highlight</label>
                        <config>
                            <type>input</type>
                            <size>50</size>
                            <max>100</max>
                            <eval>trim</eval>
                        </config>
                    </cHighlight>
                    <cCollapse>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:collapse</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:auto</label>
                                    <value>auto</value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:yes</label>
                                    <value>1</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:no</label>
                                    <value>0</value>
                                </numIndex>
                            </items>
                        </config>
                    </cCollapse>
                    <cGutter>
                        <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:gutter</label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <items>
                                <numIndex index="0">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:auto</label>
                                    <value>auto</value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:yes</label>
                                    <value>1</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>LLL:EXT:beautyofcode/Resources/Private/Language/locallang_db.xlf:no</label>
                                    <value>0</value>
                                </numIndex>
                            </items>
                        </config>
                    </cGutter>
                </el>
            </ROOT>
        </sOPTIONS>
    </sheets>
</T3DataStructure>
